-- Temperatures, 1 is reactor's max. 0.85 can cause block melts
reactor_min = 0  -- preheat to this
reactor_max = 0.1  -- shutdown at higher than
logfile = "/reactorlog"
-- Directly usable objects
reactor = peripheral.wrap "back";

supplier = peripheral.wrap "me_interface_0";
supplierside = "down";

puller = supplier
pullerside = supplierside

reactortable = dofile "reactortable"

-- Reactorstart will be called every second, 
-- and can thus be used to deadmans-trigger an external watchdog if necessary.
function reactorStart()
	rs.setOutput("back",true)
end


-- CRITICAL
function reactorStop()
	rs.setOutput("back",false)
end

